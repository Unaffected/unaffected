{
  "name": "@unaffected/app",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Cody Mercer <https://github.com/cklmercer>",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-c8": "^0.30.1",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "files": [
    "./dist",
    "./types"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": ["./types/*/index.d.ts", "./types/*.d.ts"],
      "default": ["./dist/*/index.js", "./dist/*.js"]
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./types"
      ]
    }
  }
}
